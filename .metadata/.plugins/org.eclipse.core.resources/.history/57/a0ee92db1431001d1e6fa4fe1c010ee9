package withoutstandard.service;

public enum TipoCargo {
	COORDENADOR {
		@Override
		public CalculaSalario getCargo() {
			return new ImpressaoHolerite(new Coordenador());
		}
	},
	DIRETOR {
		@Override
		public CalculaSalario getCargo() {
			return new Diretor();
		}
	},
	PROFESSOR {
		@Override
		public CalculaSalario getCargo() {
			return null;
		}
	};
	
	public abstract CalculaSalario getCargo();
}
